generator client {
   provider = "prisma-client-js"
   output   = "app/generated/prisma/client"
}

datasource db {
   provider = "mongodb"
   url      = env("DATABASE_URL")
}

model User {
   id        String @id @default(auto()) @map("_id") @db.ObjectId
   discordId String @unique

   coins Int @default(0)
   wcoin Int @default(0)
}
